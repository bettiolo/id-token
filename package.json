{
  "name": "id-token",
  "version": "0.0.3",
  "description": "OpenID Connect ID Token",
  "author": "Marco Bettiolo <marco@bettiolo.it>",
  "repository": "bettiolo/id-token",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "clean": "rm -f index.js",
    "lint": "eslint ./",
    "test": "mocha --compilers js:babel/register -R spec -b ./src/**/*.test.js",
    "compile": "babel --optional runtime src/id-token.js --out-file ./index.js",
    "test-spec": "mocha --compilers js:babel/register -R spec -b ./id-token.spec.js",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run compile && npm run test-spec"
  },
  "dependencies": {
    "babel-runtime": "^5.8.25",
    "jsonwebtoken": "^5.4.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "chai": "^3.4.0",
    "eslint": "^1.7.3",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.6.3",
    "mocha": "^2.3.3",
    "rsa-pem-from-mod-exp": "^0.8.4"
  },
  "keywords": [
    "id-token",
    "idtoken",
    "openid-connect",
    "openid"
  ],
  "publishConfig": { "registry": "https://registry.npmjs.org/" }
}
